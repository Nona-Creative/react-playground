{
  "name": "react-playground",
  "description": "A React playground for isolating and composing various React related technologies",
  "keywords": [
    "react",
    "javascript"
  ],
  "version": "0.0.1",
  "author": "NoNa Creative <hello@nona.digital>",
  "contributors": [
    "Neil Russell <neilrussell6@gmail.com>"
  ],
  "private": true,
  "homepage": "./",
  "scripts": {
    "docs:gen": "bash scripts/docs.sh generate",
    "docs:render": "bash scripts/docs.sh render",
    "npm:audit": "npm run _npm:audit:spec",
    "_npm:audit:spec": "audit-ci --critical",
    "ci:validate": "circleci config validate",
    "start": "react-scripts start",
    "_build": "react-scripts build",
    "test": "CI=true react-scripts test",
    "test:coverage": "react-scripts test --coverage",
    "test:watch": "react-scripts test --env=jsdom --watchAll",
    "lint:js": "eslint --ext .js --ignore-path .gitignore --ignore-path .eslintignore .",
    "_eject": "react-scripts eject",
    "_git:pre-commit": "npm-run-all docs:render lint:js ci:validate",
    "_git:pre-push": "npm-run-all docs:render lint:js test:coverage ci:validate",
    "help": "package-json-help.js",
    "init": "cp .env.tpl .env && npm i"
  },
  "dependencies": {
    "ramda": "^0.27.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1"
  },
  "devDependencies": {
    "@fullstackio/remark-cq": "^6.1.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "audit-ci": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "husky": "^4.2.5",
    "npm-run-all": "^4.1.5",
    "package-json-help": "0.0.10",
    "rehype-stringify": "^7.0.0",
    "remark": "^12.0.0",
    "remark-parse": "^8.0.2",
    "remark-rehype": "^6.0.0",
    "remark-stringify": "^8.0.0",
    "sinon": "^9.0.2",
    "unified": "^9.0.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run _git:pre-push",
      "pre-commit": "npm run _git:pre-commit"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
