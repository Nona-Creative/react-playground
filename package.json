{
  "name": "react-playground",
  "description": "NoNa ::: React Playground",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "scripts": {
    "help": "package-json-help.js",
    "start": "react-scripts start",
    "_build": "react-scripts build",
    "test": "CI=true react-scripts test",
    "test:coverage": "react-scripts test --coverage",
    "test:watch": "react-scripts test --env=jsdom --watchAll",
    "lint:js": "eslint --ext .js --ignore-path .gitignore .",
    "_eject": "react-scripts eject",
    "_git:pre-commit": "npm-run-all lint:js",
    "_git:pre-push": "npm-run-all lint:js test:coverage"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run _git:pre-push",
      "pre-commit": "npm run _git:pre-commit"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "js-parametrize": "0.0.2",
    "node-sass": "^4.11.0",
    "prop-types": "^15.6.2",
    "ramda": "^0.26.0",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-redux": "^6.0.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.5",
    "redux": "^4.0.1",
    "redux-first-router": "^2.1.1",
    "redux-first-router-link": "^2.1.1",
    "redux-observable": "^1.1.0",
    "sinon": "^7.2.7",
    "reselect": "^4.0.0",
    "rxjs": "^6.4.0"
  },
  "devDependencies": {
    "eslint": "^5.8.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.1.3",
    "npm-run-all": "^4.1.5",
    "package-json-help": "0.0.9"
  }
}
