{
  "name": "react-playground",
  "description": "NoNa ::: React Playground",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "scripts": {
    "start": "react-scripts start",
    "_build": "react-scripts build",
    "test": "CI=true react-scripts test",
    "test:watch": "react-scripts test --env=jsdom --watchAll",
    "_eject": "react-scripts eject",
    "lint": "eslint --ext .js --ignore-path .gitignore .",
    "npm:audit": "npm run _npm:audit:spec",
    "_npm:audit:spec": "audit-ci --critical",
    "ci:validate": "circleci config validate",
    "_git:pre-commit": "npm-run-all lint ci:validate",
    "_git:pre-push": "npm-run-all lint ci:validate",
    "help": "package-json-help.js",
    "init": "npm i && git checkout iso__base && npm i"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run _git:pre-push",
      "pre-commit": "npm run _git:pre-commit"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "dependencies": {
    "node-sass": "^4.11.0",
    "prop-types": "^15.6.2",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.5"
  },
  "devDependencies": {
    "audit-ci": "^2.1.0",
    "eslint": "^5.8.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^1.1.3",
    "npm-run-all": "^4.1.5",
    "package-json-help": "0.0.9"
  }
}
